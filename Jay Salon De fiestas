for name, ids in pairs(avatars) do
    local btn = Instance.new("TextButton", contentFrames["Ava Salon"])
    btn.Size = UDim2.new(1, -10, 0, 36)
    btn.BackgroundColor3 = Color3.fromRGB(80,80,80)
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 14
    btn.Text = name

    btn.MouseButton1Click:Connect(function()
        for i = 1, #ids, 4 do
            local group = {}
            for j = i, math.min(i+3, #ids) do
                table.insert(group, ids[j])
            end
            commandHandler:InvokeServer("hat", unpack(group))
            task.wait(0)
        end
    end)
end

local emoteBtn = Instance.new("TextButton", contentFrames["Emotes"])
emoteBtn.Size = UDim2.new(1, -10, 0, 36)
emoteBtn.BackgroundColor3 = Color3.fromRGB(80,80,80)
emoteBtn.TextColor3 = Color3.fromRGB(255,255,255)
emoteBtn.Font = Enum.Font.GothamBold
emoteBtn.TextSize = 14
emoteBtn.Text = "Load All Emotes"

emoteBtn.MouseButton1Click:Connect(function()
    loadstring(game:HttpGetAsync("https://mark-hub-h2g9.onrender.com/raw/Fe%20All%20Emotes.lua=446d7afc"))()
end)

closeButton.MouseButton1Click:Connect(function()
    mainFrame.Visible = false
end)

local iconButton = Instance.new("ImageButton", ScreenGui)
iconButton.Size = UDim2.new(0, 50, 0, 50)
iconButton.Position = UDim2.new(0, 20, 1, -70)
iconButton.BackgroundTransparency = 1
iconButton.Image = "rbxassetid://10511855986"
iconButton.Draggable = true

iconButton.MouseButton1Click:Connect(function()
    mainFrame.Visible = not mainFrame.Visible
end)

contentFrames["Ava Salon"].Visible = true
