-- // Ava Salon Buttons
for name, ids in pairs(avatars) do
    AvaTab:CreateButton({
        Name = name,
        Callback = function()
            for i = 1, #ids, 4 do
                local group = {}
                for j = i, math.min(i+3, #ids) do
                    table.insert(group, ids[j])
                end
                commandHandler:InvokeServer("hat", unpack(group))
                task.wait(0)
            end
        end
    })
end

-- // Emotes Tab
EmoteTab:CreateButton({
    Name = "Load All Emotes",
    Callback = function()
        loadstring(game:HttpGetAsync("https://mark-hub-h2g9.onrender.com/raw/Fe%20All%20Emotes.lua=446d7afc"))()
    end
})

-- // Anime Tab (Gyomei)
AnimeTab:CreateButton({
    Name = "Gyomei Anime",
    Callback = function()
        local ids = {
            17500249916, 105402325730547, 119293771649840,
            73685635695979, 111482005838500, 79659094933189, 128246037870792,
            98117921837732, 115087564644652, 86625849012944, 112754538091710,
            112951529531094, 110745408535840, 83277367324299,
            82197908379724, 130031201539866, 120812609142671, 97300497165976,
            18234058046, 103174378935537, 84357765892373
        }
        for i = 1, #ids, 4 do
            local group = {}
            for j = i, math.min(i+3, #ids) do
                table.insert(group, ids[j])
            end
            commandHandler:InvokeServer("hat", unpack(group))
            task.wait(0)
        end
    end
})
