-- // Load Rayfield
local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

local Window = Rayfield:CreateWindow({
    Name = "Jay Corryfc",
    LoadingTitle = "Jay Corryfc",
    LoadingSubtitle = "Salon De Fiestas",
    ConfigurationSaving = {
        Enabled = false
    },
    Discord = {
        Enabled = false
    },
    KeySystem = false,
})

-- // Tabs
local AvaTab = Window:CreateTab("Ava Salon", 8670534868) -- Ava Salon
local EmoteTab = Window:CreateTab("Emotes", 8670621484) -- Emotes
local AnimeTab = Window:CreateTab("Ava Anime", 8670510028) -- Anime

-- // Handler
local commandHandler = game:GetService("ReplicatedStorage")
    :WaitForChild("Connections")
    :WaitForChild("dataProviders")
    :WaitForChild("commandHandler")

